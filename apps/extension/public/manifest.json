{
  "name": "AK Crawl Agent",
  "description": "An extension to crawl web pages",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "32": "web-crawler.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
